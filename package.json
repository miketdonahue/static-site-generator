{
  "name": "static-site-generator",
  "version": "1.0.0",
  "description": "A static site generator using Webpack and Handlebars",
  "author": "Mike Donahue",
  "license": "MIT",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack -d --config webpack.dev.js",
    "build": "NODE_ENV=production webpack -p --config webpack.prod.js",
    "start:dev": "webpack-dev-server --color --config webpack.dev.js",
    "start:prod": "pushd public/; python -m SimpleHTTPServer 8080; popd"
  },
  "dependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.10.0",
    "node-sass": "^4.7.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "simple-progress-webpack-plugin": "^1.0.4",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
}
